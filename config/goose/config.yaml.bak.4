OLLAMA_HOST: http://localhost:11434/
extensions:
  cognee:
    args:
    - run
    - --rm
    - -i
    - --volume
    - agentgoose_cognee_data:/app/data
    - --volume
    - agentgoose_cognee_db:/app/database
    - --volume
    - agentgoose_cognee_logs:/app/logs
    - agentgoose-cognee-mcp
    bundled: null
    cmd: docker
    description: null
    enabled: true
    env_keys: []
    envs: {}
    name: Cognee
    timeout: 300
    type: stdio
  computercontroller:
    bundled: true
    description: null
    display_name: Computer Controller
    enabled: true
    name: computercontroller
    timeout: 300
    type: builtin
  developer:
    bundled: true
    description: null
    display_name: Developer Tools
    enabled: true
    name: developer
    timeout: 300
    type: builtin
  memory:
    bundled: true
    description: null
    display_name: Memory
    enabled: true
    name: memory
    timeout: 300
    type: builtin
GOOSE_PROVIDER: ollama
GOOSE_MODEL: mistral-nemo:12b
