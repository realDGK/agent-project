GOOSE_MODEL: gemini-2.5-flash-preview-05-20
GOOSE_PROVIDER: google
OLLAMA_HOST: http://localhost:11434/
OLLAMA_CONTEXT_LENGTH: 32768
extensions:
  cognee:
    args:
    - exec
    - -i
    - goose_cognee_mcp
    - python
    - -m
    - src.server
    - --transport
    - stdio
    bundled: null
    cmd: docker
    description: "Cognee Memory Database"
    enabled: true
    env_keys: []
    envs: {}
    name: Cognee
    timeout: 300
    type: stdio
  computercontroller:
    bundled: true
    description: null
    display_name: Computer Controller
    enabled: true
    name: computercontroller
    timeout: 300
    type: builtin
  developer:
    bundled: true
    description: null
    display_name: Developer Tools
    enabled: true
    name: developer
    timeout: 300
    type: builtin
  memory:
    bundled: true
    description: null
    display_name: Memory
    enabled: true
    name: memory
    timeout: 300
    type: builtin
