{
  "title": "Proof of Publication/Mailing Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, typically 'Affidavit of Publication'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Publisher", "Declarant", "Mailing Service"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "proof_of_service_details": {
      "type": "object",
      "properties": {
        "service_type": { "type": "string", "enum": ["Publication", "Mailing"] },
        "document_served": { "type": "string" },
        "publication_name": { "type": "string" },
        "publication_dates": { "type": "array", "items": { "type": "string" } },
        "mailing_date": { "type": "string", "format": "date" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "proof_of_service_details"]
}