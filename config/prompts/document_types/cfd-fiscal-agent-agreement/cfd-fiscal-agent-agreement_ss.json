{
  "title": "CFD Fiscal Agent Agreement Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, typically 'Fiscal Agent Agreement'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Public Agency", "City", "Fiscal Agent"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "cfd_details": {
      "type": "object",
      "properties": {
        "cfd_number": { "type": "string" },
        "agent_responsibilities": { "type": "string" },
        "agent_compensation": { "type": "string" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "cfd_details"]
}