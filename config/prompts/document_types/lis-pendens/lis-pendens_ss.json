{
  "title": "Lis Pendens Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, typically 'Notice of Pendency of Action' or 'Lis Pendens'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Plaintiff", "Defendant"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "litigation_details": {
      "type": "object",
      "properties": {
        "court_name": { "type": "string" },
        "case_number": { "type": "string" },
        "property_affected": { "type": "string" },
        "nature_of_action": { "type": "string" }
      }
    },
    "legal_recording_details": {
      "type": "object",
      "properties": {
        "instrument_number": { "type": "string" },
        "recording_date": { "type": "string", "format": "date" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "litigation_details"]
}