{
  "title": "HOA Bylaws Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'Bylaws of [HOA Name]'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Homeowners Association"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "governance_details": {
      "type": "object",
      "properties": {
        "director_quorum_requirement": { "type": "string" },
        "member_quorum_requirement": { "type": "string" },
        "director_count": { "type": "string" },
        "officer_titles": { "type": "array", "items": { "type": "string" } },
        "fiscal_year_end": { "type": "string" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "parties", "governance_details"]
}