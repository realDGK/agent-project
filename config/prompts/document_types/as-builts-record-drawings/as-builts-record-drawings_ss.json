{
  "title": "As-Built/Record Drawing Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The title of the plan set, e.g., 'Record Drawings'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Contractor", "Architect", "Engineer"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "plan_details": {
      "type": "object",
      "properties": {
        "project_name": { "type": "string" },
        "plan_type": { "type": "string", "default": "As-Builts/Record Drawings" },
        "certification_statement": { "type": "string" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "parties", "plan_details"]
}