{
  "title": "EIR (Draft/Final) Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'Draft Environmental Impact Report'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Lead Agency", "Project Applicant"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "environmental_details": {
      "type": "object",
      "properties": {
        "document_type": { "type": "string", "enum": ["Draft EIR", "Final EIR"] },
        "project_name": { "type": "string" },
        "significant_unavoidable_impacts": { "type": "array", "items": { "type": "string" } },
        "alternatives_analyzed": { "type": "array", "items": { "type": "string" } }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "environmental_details"]
}