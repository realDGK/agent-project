{
  "title": "Initial Study (IS) Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, typically 'Initial Study'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Lead Agency", "Project Applicant"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "environmental_details": {
      "type": "object",
      "properties": {
        "document_type": { "type": "string", "default": "Initial Study" },
        "project_name": { "type": "string" },
        "environmental_determination": { "type": "string", "description": "The conclusion of the study, e.g., 'Potentially Significant Impact', 'Less Than Significant Impact with Mitigation'." },
        "mitigation_measures_summary": { "type": "string" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "environmental_details"]
}