{
  "title": "Notice of Default (Loan) Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, typically 'Notice of Default'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Lender", "Borrower"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "default_notice_details": {
      "type": "object",
      "properties": {
        "loan_reference": { "type": "string" },
        "nature_of_default": { "type": "string" },
        "amount_to_cure": { "type": "number" },
        "cure_period": { "type": "string" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "default_notice_details"]
}