{
  "title": "Bill of Sale Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, typically 'Bill of Sale'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Seller", "Buyer"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "bill_of_sale_details": {
      "type": "object",
      "properties": {
        "personal_property_description": { "type": "string" },
        "purchase_price": { "type": "number" },
        "as_is_clause": { "type": "string" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "bill_of_sale_details"]
}