{
  "title": "Monument Record Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string"
    },
    "effective_date": {
      "type": "string",
      "format": "date"
    },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "role": {
            "type": "string",
            "enum": [
              "Surveyor",
              "Public Agency"
            ]
          }
        },
        "required": [
          "name",
          "role"
        ]
      }
    },
    "survey_details": {
      "type": "object",
      "properties": {
        "monument_description": {
          "type": "string"
        },
        "monument_location": {
          "type": "string"
        },
        "surveyor_license_number": {
          "type": "string"
        }
      }
    }
  },
  "required": [
    "contract_name",
    "effective_date",
    "parties",
    "survey_details"
  ]
}