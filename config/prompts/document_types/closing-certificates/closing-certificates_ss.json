{
  "title": "Closing Certificate Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'Seller's Closing Certificate'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Certifying Party", "Beneficiary"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "certificate_details": {
      "type": "object",
      "properties": {
        "certifications_made": { "type": "array", "items": { "type": "string" } }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "certificate_details"]
}