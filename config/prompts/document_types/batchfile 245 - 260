  "rent_roll_details": {
    "type": "object",
    "description": "Details specific to a property rent roll.",
    "properties": {
      "property_name": { "type": "string" },
      "total_units": { "type": "integer" },
      "occupied_units": { "type": "integer" },
      "occupancy_rate_pct": { "type": "number" },
      "total_monthly_rent": { "type": "number" },
