{
  "title": "Lease Estoppel Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'Tenant Estoppel Certificate'."
    },
    "effective_date": {
      "type": "string",
      "format": "date",
      "description": "The date the certificate was signed by the tenant."
    },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Tenant", "Landlord", "Lender", "Buyer"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "estoppel_details": {
      "type": "object",
      "properties": {
        "lease_date": { "type": "string", "format": "date" },
        "lease_premises": { "type": "string" },
        "rent_commencement_date": { "type": "string", "format": "date" },
        "lease_expiration_date": { "type": "string", "format": "date" },
        "current_monthly_rent": { "type": "number" },
        "security_deposit_amount": { "type": "number" },
        "known_defaults": { "type": "string" },
        "outstanding_allowances": { "type": "string" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "estoppel_details"]
}