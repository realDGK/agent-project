{
  "title": "Rules & Regulations Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'Building Rules and Regulations'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Landlord", "Property Manager"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "rules_and_regulations_details": {
      "type": "object",
      "properties": {
        "property_name": { "type": "string" },
        "key_rules": { "type": "array", "items": { "type": "string" } }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "parties", "rules_and_regulations_details"]
}