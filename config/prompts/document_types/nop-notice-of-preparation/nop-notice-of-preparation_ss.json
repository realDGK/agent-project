{
  "title": "Notice of Preparation (NOP) Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, typically 'Notice of Preparation of a Draft Environmental Impact Report'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Lead Agency", "Project Applicant"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "environmental_details": {
      "type": "object",
      "properties": {
        "document_type": { "type": "string", "default": "NOP" },
        "project_name": { "type": "string" },
        "project_description": { "type": "string" },
        "scoping_meeting_date": { "type": "string", "format": "date" },
        "comment_period_end_date": { "type": "string", "format": "date" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "environmental_details"]
}