{
  "title": "Agricultural Lease/Crop Rights Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'Agricultural Lease'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Lessor", "Lessee"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "lease_details": {
      "type": "object",
      "properties": {
        "lease_type": { "type": "string", "default": "Agricultural" },
        "property_description": { "type": "string" },
        "lease_term": { "type": "string" },
        "rent_terms": { "type": "string" },
        "permitted_crops": { "type": "string" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "lease_details"]
}