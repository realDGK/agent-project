{
  "title": "Health Risk Assessment (HRA) Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'Health Risk Assessment'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Author", "Consultant", "Client"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "technical_study_details": {
      "type": "object",
      "properties": {
        "study_type": { "type": "string", "default": "Health Risk Assessment" },
        "project_name": { "type": "string" },
        "pollutants_analyzed": { "type": "array", "items": { "type": "string" } },
        "risk_conclusion": { "type": "string" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "technical_study_details"]
}