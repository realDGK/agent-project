{
  "title": "Notice of Trustee's Sale Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string"
    },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "role": {
            "type": "string",
            "enum": [
              "Trustor",
              "Beneficiary",
              "Trustee"
            ]
          }
        },
        "required": [
          "name",
          "role"
        ]
      }
    },
    "foreclosure_details": {
      "type": "object",
      "properties": {
        "property_address": {
          "type": "string"
        },
        "apn": {
          "type": "string"
        },
        "sale_date": {
          "type": "string",
          "format": "date"
        },
        "sale_time": {
          "type": "string"
        },
        "sale_location": {
          "type": "string"
        },
        "unpaid_balance": {
          "type": "number"
        }
      }
    }
  },
  "required": [
    "contract_name",
    "parties",
    "foreclosure_details"
  ]
}