{
  "title": "Mechanics Lien Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, typically 'Mechanic's Lien'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Claimant", "Owner"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "mechanics_lien_details": {
      "type": "object",
      "properties": {
        "property_address": { "type": "string" },
        "amount_of_lien": { "type": "number" }
      }
    },
    "legal_recording_details": {
      "type": "object",
      "properties": {
        "instrument_number": { "type": "string" },
        "recording_date": { "type": "string", "format": "date" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "parties", "mechanics_lien_details"]
}