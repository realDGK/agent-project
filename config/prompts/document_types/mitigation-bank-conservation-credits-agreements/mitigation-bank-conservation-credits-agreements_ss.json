{
  "title": "Mitigation/Conservation Credit Agreement Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'Credit Sale Agreement'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Seller", "Bank Owner", "Buyer", "Permittee"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "mitigation_credit_details": {
      "type": "object",
      "properties": {
        "bank_name": { "type": "string" },
        "credit_type": { "type": "string" },
        "number_of_credits": { "type": "number" },
        "price_per_credit": { "type": "number" },
        "total_purchase_price": { "type": "number" },
        "project_to_be_mitigated": { "type": "string" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "mitigation_credit_details"]
}