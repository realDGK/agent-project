{
  "title": "Sign Permit Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, typically 'Sign Permit'."
    },
    "effective_date": {
      "type": "string",
      "format": "date",
      "description": "The date the permit was issued."
    },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Permittee", "Applicant", "Issuing Agency"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "permit_details": {
      "type": "object",
      "properties": {
        "permit_number": { "type": "string" },
        "property_address": { "type": "string" },
        "sign_description": { "type": "string" },
        "sign_dimensions": { "type": "string" },
        "expiration_date": { "type": "string", "format": "date" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "permit_details"]
}