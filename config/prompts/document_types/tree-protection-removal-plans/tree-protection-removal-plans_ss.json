{
  "title": "Tree Plan Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string"
    },
    "effective_date": {
      "type": "string",
      "format": "date"
    },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "role": {
            "type": "string",
            "enum": [
              "Developer",
              "Arborist",
              "City"
            ]
          }
        },
        "required": [
          "name",
          "role"
        ]
      }
    },
    "plan_details": {
      "type": "object",
      "properties": {
        "project_name": {
          "type": "string"
        },
        "plan_type": {
          "type": "string"
        },
        "trees_to_be_removed_summary": {
          "type": "string"
        },
        "trees_to_be_protected_summary": {
          "type": "string"
        }
      }
    }
  },
  "required": [
    "contract_name",
    "effective_date",
    "parties",
    "plan_details"
  ]
}