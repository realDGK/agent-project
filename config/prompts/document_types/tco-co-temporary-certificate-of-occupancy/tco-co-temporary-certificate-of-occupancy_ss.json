{
  "title": "Certificate of Occupancy Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'Certificate of Occupancy'."
    },
    "effective_date": {
      "type": "string",
      "format": "date",
      "description": "The date the certificate was issued."
    },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Owner", "Issuing Agency", "Building Official"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "occupancy_certificate_details": {
      "type": "object",
      "properties": {
        "certificate_type": { "type": "string", "enum": ["Temporary", "Final"] },
        "permit_number": { "type": "string" },
        "property_address": { "type": "string" },
        "description_of_occupancy": { "type": "string" },
        "expiration_date": { "type": "string", "format": "date" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "occupancy_certificate_details"]
}