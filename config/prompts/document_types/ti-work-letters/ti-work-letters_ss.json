{
  "title": "TI Work Letter Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, typically 'Work Letter'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Landlord", "Tenant"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "ti_work_letter_details": {
      "type": "object",
      "properties": {
        "landlord_work_summary": { "type": "string" },
        "tenant_work_summary": { "type": "string" },
        "tenant_improvement_allowance": { "type": "number" },
        "delivery_condition": { "type": "string" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "parties", "ti_work_letter_details"]
}