JSON

{
  "survey_details": {
    "type": "object",
    "description": "Details specific to a land survey.",
    "properties": {
      "property_address": { "type": "string" },
      "property_legal_description": { "type": "string" },
      "apn": { "type": "string" },
      "flood_zone_information": { "type": "string" },
