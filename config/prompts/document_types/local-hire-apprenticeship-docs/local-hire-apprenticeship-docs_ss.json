{
  "title": "Local Hire/Apprenticeship Document Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'Local Hire Compliance Report'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Contractor", "Public Agency"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "compliance_report_details": {
      "type": "object",
      "properties": {
        "project_name": { "type": "string" },
        "reporting_period": { "type": "string" },
        "local_hire_requirement_pct": { "type": "number" },
        "local_hire_achieved_pct": { "type": "number" },
        "apprentice_utilization_requirement_pct": { "type": "number" },
        "apprentice_utilization_achieved_pct": { "type": "number" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "parties", "compliance_report_details"]
}