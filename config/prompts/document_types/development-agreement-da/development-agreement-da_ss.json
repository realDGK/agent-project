{
  "title": "Development Agreement (DA) Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'Development Agreement By and Between...'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Developer", "Public Agency", "City", "County"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "development_agreement_details": {
      "type": "object",
      "properties": {
        "project_description": { "type": "string" },
        "term_of_agreement": { "type": "string" },
        "vested_rights": { "type": "string" },
        "developer_obligations": { "type": "string" },
        "public_benefits": { "type": "string" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "development_agreement_details"]
}