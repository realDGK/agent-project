{
  "title": "Decision Log Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, typically 'Decision Log'."
    },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Project Manager", "Owner"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "decision_log_details": {
      "type": "object",
      "properties": {
        "project_name": { "type": "string" },
        "decisions": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "decision_id": { "type": "string" },
              "date": { "type": "string", "format": "date" },
              "decision": { "type": "string" },
              "reason": { "type": "string" }
            }
          }
        }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "parties", "decision_log_details"]
}