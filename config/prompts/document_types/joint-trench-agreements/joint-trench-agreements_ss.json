{
  "title": "Joint Trench Agreement Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, typically 'Joint Trench Agreement'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Developer", "Utility Company"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "utility_service_details": {
      "type": "object",
      "properties": {
        "project_name": { "type": "string" },
        "participating_utilities": { "type": "array", "items": { "type": "string" } },
        "cost_sharing_arrangement": { "type": "string" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "utility_service_details"]
}