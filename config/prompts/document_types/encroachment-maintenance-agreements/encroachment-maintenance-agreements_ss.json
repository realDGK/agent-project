{
  "title": "Encroachment Maintenance Agreement Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string"
    },
    "effective_date": {
      "type": "string",
      "format": "date"
    },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "role": {
            "type": "string",
            "enum": [
              "Property Owner",
              "Encroaching Party",
              "Public Agency"
            ]
          }
        },
        "required": [
          "name",
          "role"
        ]
      }
    },
    "encroachment_details": {
      "type": "object",
      "properties": {
        "description_of_encroachment": {
          "type": "string"
        },
        "location_of_encroachment": {
          "type": "string"
        },
        "maintenance_responsibility": {
          "type": "string"
        },
        "is_revocable": {
          "type": "boolean"
        }
      }
    }
  },
  "required": [
    "contract_name",
    "effective_date",
    "parties",
    "encroachment_details"
  ]
}