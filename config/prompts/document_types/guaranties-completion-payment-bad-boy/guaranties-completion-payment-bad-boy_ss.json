{
  "title": "Guaranty Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'Completion Guaranty'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Guarantor", "Lender", "Borrower"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "guaranty_details": {
      "type": "object",
      "properties": {
        "guaranty_type": { "type": "string" },
        "guaranteed_obligation": { "type": "string" },
        "maximum_liability": { "type": "string" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "guaranty_details"]
}