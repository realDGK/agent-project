{
  "title": "Marketing Brochure/Flyer Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'Property Flyer'."
    },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Listing Broker"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "marketing_material_details": {
      "type": "object",
      "properties": {
        "property_name": { "type": "string" },
        "property_address": { "type": "string" },
        "property_type": { "type": "string" },
        "key_features": { "type": "array", "items": { "type": "string" } }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "parties", "marketing_material_details"]
}