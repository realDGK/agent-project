{
  "title": "Specific Plan Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'The Riverfront Specific Plan'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Approving Agency"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "entitlement_details": {
      "type": "object",
      "properties": {
        "plan_area_description": { "type": "string" },
        "plan_summary": { "type": "string" },
        "allowed_land_uses": { "type": "array", "items": { "type": "string" } },
        "development_standards_summary": { "type": "string" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "entitlement_details"]
}