{
  "title": "Staff Report Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'Staff Report to the Planning Commission'."
    },
    "effective_date": {
      "type": "string",
      "format": "date",
      "description": "The date of the meeting for which the report was prepared."
    },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Authoring Agency", "Reviewing Body", "Applicant"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "staff_report_details": {
      "type": "object",
      "properties": {
        "project_name": { "type": "string" },
        "application_number": { "type": "string" },
        "staff_recommendation": { "type": "string" },
        "project_summary": { "type": "string" },
        "key_issues": { "type": "array", "items": { "type": "string" } }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "staff_report_details"]
}