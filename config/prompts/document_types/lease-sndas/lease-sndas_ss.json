{
  "title": "Lease SNDA Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'Subordination, Non-Disturbance, and Attornment Agreement'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Tenant", "Landlord", "Lender"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "snda_details": {
      "type": "object",
      "properties": {
        "lease_date": { "type": "string", "format": "date" },
        "deed_of_trust_date": { "type": "string", "format": "date" },
        "subordination_clause": { "type": "string" },
        "non_disturbance_clause": { "type": "string" },
        "attornment_clause": { "type": "string" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "snda_details"]
}