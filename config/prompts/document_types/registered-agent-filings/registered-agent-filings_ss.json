{
  "title": "Registered Agent Filing Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the filing, e.g., 'Statement of Change of Registered Agent'."
    },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Entity", "New Registered Agent", "Former Registered Agent", "Authorized Person"]
          },
          "address": { "type": "string" }
        },
        "required": ["name", "role"]
      }
    },
    "legal_recording_details": {
      "type": "object",
      "properties": {
        "state": { "type": "string" },
        "filing_date": { "type": "string", "format": "date" }
      }
    },
    "registered_agent_filing_details": {
      "type": "object",
      "properties": {
        "entity_file_number": {
          "type": "string",
          "description": "The official file number assigned by the Secretary of State."
        },
        "filing_type": {
          "type": "string",
          "description": "The nature of the filing (e.g., 'Change of Agent', 'Resignation of Agent', 'Appointment of Agent')."
        }
      }
    },
    "discovered_entities": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "entity_name": { "type": "string" },
          "entity_value": { "type": "string" },
          "context_summary": { "type": "string" }
        }
      }
    }
  },
  "required": ["contract_name", "parties"]
}