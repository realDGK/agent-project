{
  "title": "Lease Guaranty Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, typically 'Guaranty of Lease'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Guarantor", "Landlord", "Tenant"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "guaranty_details": {
      "type": "object",
      "properties": {
        "guaranteed_lease_date": { "type": "string", "format": "date" },
        "guaranteed_obligations": { "type": "string" },
        "is_unconditional_and_absolute": { "type": "boolean" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "guaranty_details"]
}