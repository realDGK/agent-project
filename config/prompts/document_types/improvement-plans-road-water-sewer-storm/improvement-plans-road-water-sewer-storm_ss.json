{
  "title": "Improvement Plans Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The title of the plan set, e.g., 'Tract 8888 - Street Improvement Plans'."
    },
    "effective_date": {
      "type": "string",
      "format": "date",
      "description": "The date the plans were approved or signed."
    },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Developer", "Engineer", "Approving Agency"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "plan_details": {
      "type": "object",
      "properties": {
        "project_name": { "type": "string" },
        "plan_type": { "type": "string", "description": "e.g., 'Road', 'Water', 'Sewer', 'Storm Drain'." },
        "engineer_of_record": { "type": "string" },
        "approval_date": { "type": "string", "format": "date" },
        "plan_sheet_count": { "type": "integer" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "parties", "plan_details"]
}