{
  "title": "Meeting Agenda/Minutes Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'Agenda for OAC Meeting #25'."
    },
    "effective_date": {
      "type": "string",
      "format": "date",
      "description": "The date of the meeting."
    },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Attendee", "Author"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "meeting_minutes_details": {
      "type": "object",
      "properties": {
        "document_type": { "type": "string", "enum": ["Agenda", "Minutes"] },
        "project_name": { "type": "string" },
        "meeting_title": { "type": "string" },
        "agenda_items": { "type": "array", "items": { "type": "string" } },
        "key_decisions_made": { "type": "array", "items": { "type": "string" } }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "meeting_minutes_details"]
}