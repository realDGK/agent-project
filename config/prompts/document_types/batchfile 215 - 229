    { "name": "City of Oakwood", "role": "Public Agency" },
    { "name": "Apex Civil Engineering", "role": "Engineer" }
  ],
  "cfd_details": {
    "cfd_name": "Public Services",
    "cfd_number": "2032-1",
    "map_summary": "Shows the boundaries of the CFD, which includes the 300-lot Oakwood Hills subdivision."
  },
  "legal_recording_details": {
    "instrument_number": "2032-111222",
    "recording_date": "2032-04-01"
  },
  "discovered_entities": []
