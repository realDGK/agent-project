  ],
  "environmental_details": {
    "document_type": "MMRP",
    "project_name": "Elm Street Apartments Project",
    "mitigation_measures": [
      {
        "measure_id": "BIO-1",
        "measure_description": "A qualified biologist shall conduct a pre-construction survey for nesting birds.",
        "monitoring_responsibility": "City Planning Department",
        "timing": "Within 14 days prior to any ground disturbance."
      },
      {
        "measure_id": "NOI-1",
        "measure_description": "Construction activities shall be limited to the hours of 7 AM to 6 PM.",
        "monitoring_responsibility": "City Code Enforcement",
        "timing": "Throughout construction."
      }
    ]
  },
