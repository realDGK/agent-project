{
  "title": "Assessment District Diagram Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, typically 'Assessment Diagram'."
    },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Engineer", "Public Agency"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "assessment_district_details": {
      "type": "object",
      "properties": {
        "district_name": { "type": "string" },
        "diagram_summary": { "type": "string" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "parties", "assessment_district_details"]
}