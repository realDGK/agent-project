{
  "title": "Capacity/Allocation Agreement & Transfer Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'Sewer Capacity Transfer Agreement'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Seller", "Buyer", "Utility District"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "utility_service_details": {
      "type": "object",
      "properties": {
        "utility_type": { "type": "string" },
        "capacity_amount": { "type": "string" },
        "unit_of_measure": { "type": "string" },
        "purchase_price": { "type": "number" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "utility_service_details"]
}