  ],
  "subcontract_details": {
    "project_name": "Downtown Tower",
    "scope_of_work": "Furnish and install all electrical work for the project in accordance with the prime contract and plans.",
    "subcontract_price": 5000000,
    "payment_terms": "Monthly based on percentage of work completed, less 10% retention."
  },
  "discovered_entities": []
}
