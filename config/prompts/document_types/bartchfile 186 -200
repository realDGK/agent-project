    { "name": "Global Architects Inc.", "role": "Architect" }
  ],
  "design_contract_details": {
    "project_name": "Downtown Tower, 123 Main Street",
    "basic_services_scope": "Schematic Design, Design Development, Construction Documents, Bidding or Negotiation, and Construction Phase Services.",
    "supplemental_services_scope": null,
    "architects_compensation": "Stipulated sum of $5,000,000 for Basic Services."
  },
  "discovered_entities": []
}
