{
  "title": "Subcontract Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'Subcontract Agreement'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Contractor", "Subcontractor"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "subcontract_details": {
      "type": "object",
      "properties": {
        "project_name": { "type": "string" },
        "scope_of_work": { "type": "string" },
        "subcontract_price": { "type": "number" },
        "payment_terms": { "type": "string" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "subcontract_details"]
}