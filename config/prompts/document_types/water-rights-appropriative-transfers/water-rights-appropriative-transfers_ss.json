{
  "title": "Water Rights/Appropriative Transfer Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'Deed of Water Right'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Grantor", "Grantee"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "water_rights_details": {
      "type": "object",
      "properties": {
        "water_right_permit_or_license_number": { "type": "string" },
        "amount_of_water_transferred": { "type": "string" },
        "source_of_water": { "type": "string" },
        "place_of_use": { "type": "string" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "water_rights_details"]
}