{
  "title": "Stormwater Inspection Report Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, typically 'Stormwater Inspection Report'."
    },
    "effective_date": {
      "type": "string",
      "format": "date",
      "description": "The date of the inspection."
    },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Inspector", "QSP"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "stormwater_details": {
      "type": "object",
      "properties": {
        "project_name": { "type": "string" },
        "wdid_number": { "type": "string" },
        "inspection_findings": { "type": "string" },
        "corrective_actions_required": { "type": "string" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "stormwater_details"]
}