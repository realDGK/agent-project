{
  "title": "CAM Budget/Reconciliation Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., '2033 CAM Reconciliation'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Landlord", "Property Manager", "Tenant"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "cam_details": {
      "type": "object",
      "properties": {
        "property_name": { "type": "string" },
        "period": { "type": "string" },
        "document_type": { "type": "string", "enum": ["Budget", "Reconciliation"] },
        "total_actual_expenses": { "type": "number" },
        "total_estimated_payments": { "type": "number" },
        "tenant_pro_rata_share_pct": { "type": "number" },
        "tenant_charge_or_credit": { "type": "number" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "parties", "cam_details"]
}