{
  "title": "FEMA LOMR/LOMA Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'Letter of Map Revision (LOMR)'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Applicant", "Issuing Agency"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "flood_details": {
      "type": "object",
      "properties": {
        "case_number": { "type": "string" },
        "document_type": { "type": "string", "enum": ["LOMR", "LOMA"] },
        "community_name": { "type": "string" },
        "map_panel_affected": { "type": "string" },
        "determination": { "type": "string" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "flood_details"]
}