{
  "title": "1099/W-9 Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'Form W-9', 'Form 1099-MISC'."
    },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Payor", "Recipient", "Requestor"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "tax_details": {
      "type": "object",
      "properties": {
        "form_reference": { "type": "string" },
        "taxpayer_id_number": { "type": "string" },
        "total_payment_amount": { "type": "number" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "parties", "tax_details"]
}