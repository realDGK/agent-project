{
  "title": "Aerial/Map Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'Site Aerial'."
    },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Creator", "Broker"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "map_details": {
      "type": "object",
      "properties": {
        "map_type": { "type": "string", "enum": ["Aerial", "Parcel Map", "Location Map"] },
        "property_address": { "type": "string" },
        "labels_or_callouts": { "type": "array", "items": { "type": "string" } }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "map_details"]
}