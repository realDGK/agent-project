{
  "title": "Planned Development (PUD) Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'Ordinance Approving Planned Development Permit PUD-123'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Applicant", "Approving Agency"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "entitlement_details": {
      "type": "object",
      "properties": {
        "application_number": { "type": "string" },
        "project_name": { "type": "string" },
        "property_description": { "type": "string" },
        "pud_summary": { "type": "string", "description": "A summary of the approved development plan and any deviations from standard zoning." }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "entitlement_details"]
}