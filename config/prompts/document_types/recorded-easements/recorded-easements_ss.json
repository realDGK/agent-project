{
  "title": "Easement Agreement Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'Easement Agreement'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Grantor", "Grantee"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "easement_details": {
      "type": "object",
      "properties": {
        "easement_type": { "type": "string" },
        "purpose": { "type": "string" },
        "servient_tenement_description": { "type": "string" },
        "dominant_tenement_description": { "type": "string" },
        "is_appurtenant_or_in_gross": { "type": "string" }
      }
    },
    "legal_recording_details": {
      "type": "object",
      "properties": {
        "instrument_number": { "type": "string" },
        "recording_date": { "type": "string", "format": "date" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "easement_details"]
}