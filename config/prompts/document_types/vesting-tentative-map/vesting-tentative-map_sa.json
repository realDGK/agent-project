{
  "map_details": {
    "type": "object",
    "description": "Details specific to maps, plats, and legal descriptions.",
    "properties": {
      "property_legal_description": { "type": "string" },
      "apn": { "type": "string" },
      "county": { "type": "string" },
      "state": { "type": "string" },
      "map_type": { "type": "string", "enum": ["Tentative Tract Map", "Final Tract Map", "Parcel Map", "Vesting Tentative Map"] },
      "map_number": { "type": "string" },
      "number_of_lots_created": { "type": "integer" },
      "property_address": { "type": "string" },
      "approval_date": { "type": "string", "format": "date" },
      "expiration_date": { "type": "string", "format": "date" },
      "vesting_rights_summary": { "type": "string" }
    }
  }
}