{
  "title": "Daily Report Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, typically 'Daily Construction Report'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Superintendent", "Contractor"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "daily_report_details": {
      "type": "object",
      "properties": {
        "project_name": { "type": "string" },
        "weather": { "type": "string" },
        "work_performed": { "type": "string" },
        "subcontractors_on_site": { "type": "array", "items": { "type": "string" } },
        "equipment_on_site": { "type": "array", "items": { "type": "string" } },
        "issues_or_delays": { "type": "string" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "daily_report_details"]
}