{
  "title": "Tenant Compliance Notice Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'Notice to Cure or Quit'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Landlord", "Tenant"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "compliance_notice_details": {
      "type": "object",
      "properties": {
        "property_address": { "type": "string" },
        "unit_number": { "type": "string" },
        "notice_type": { "type": "string" },
        "nature_of_violation": { "type": "string" },
        "cure_period": { "type": "string" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "compliance_notice_details"]
}