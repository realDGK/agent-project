{
  "title": "Final Map/Parcel Map Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'Final Map of Tract No. 5555'."
    },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Owner", "Subdivider", "Engineer", "City Clerk", "County Recorder"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "map_details": {
      "type": "object",
      "properties": {
        "map_type": { "type": "string", "enum": ["Final Map", "Parcel Map"] },
        "map_number": { "type": "string" },
        "number_of_lots_created": { "type": "integer" }
      }
    },
    "legal_recording_details": {
      "type": "object",
      "properties": {
        "instrument_number": { "type": "string" },
        "recording_date": { "type": "string", "format": "date" },
        "book_and_page": { "type": "string" },
        "county": { "type": "string" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "parties", "map_details", "legal_recording_details"]
}