{
  "title": "RWQCB 401 Water Quality Certification Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'Clean Water Act Section 401 Water Quality Certification'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Applicant", "Issuing Agency"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "permit_details": {
      "type": "object",
      "properties": {
        "certification_number": { "type": "string" },
        "project_name": { "type": "string" },
        "related_404_permit": { "type": "string" },
        "key_conditions": { "type": "array", "items": { "type": "string" } }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "permit_details"]
}