{
  "title": "CM/GC Agreement Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'Agreement Between Owner and Construction Manager'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Owner", "Construction Manager", "General Contractor"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "construction_contract_details": {
      "type": "object",
      "properties": {
        "project_name": { "type": "string" },
        "contract_type": { "type": "string" },
        "preconstruction_phase_services": { "type": "string" },
        "construction_phase_services": { "type": "string" },
        "compensation_structure": { "type": "string" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "construction_contract_details"]
}