    "director_quorum_requirement": "A majority of the directors.",
    "member_quorum_requirement": "Fifty-one percent (51%) of the total voting power.",
    "director_count": "Five (5)",
    "officer_titles": null,
    "fiscal_year_end": "December 31"
  },
  "discovered_entities": []
}
