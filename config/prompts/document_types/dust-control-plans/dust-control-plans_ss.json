{
  "title": "Dust Control Plan Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, typically 'Dust Control Plan'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Developer", "Contractor", "Regulatory Agency"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "dust_control_plan_details": {
      "type": "object",
      "properties": {
        "project_name": { "type": "string" },
        "project_acreage": { "type": "number" },
        "best_management_practices_bmps": { "type": "array", "items": { "type": "string" } }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "parties", "dust_control_plan_details"]
}