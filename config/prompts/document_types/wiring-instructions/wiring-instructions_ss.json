{
  "title": "Wiring Instructions Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, typically 'Wire Transfer Instructions'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Beneficiary", "Sending Party"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "wire_details": {
      "type": "object",
      "properties": {
        "beneficiary_bank": { "type": "string" },
        "routing_number_aba": { "type": "string" },
        "account_number": { "type": "string" },
        "swift_code": { "type": "string" },
        "reference_info": { "type": "string" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "parties", "wire_details"]
}