{
  "title": "ADU Program Document Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'Accessory Dwelling Unit Permit Application'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Applicant", "Property Owner", "Issuing Agency"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "adu_details": {
      "type": "object",
      "properties": {
        "property_address": { "type": "string" },
        "adu_type": { "type": "string", "enum": ["Detached", "Attached", "Conversion", "JADU"] },
        "adu_square_footage": { "type": "integer" },
        "deed_restriction_required": { "type": "boolean" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "parties", "adu_details"]
}