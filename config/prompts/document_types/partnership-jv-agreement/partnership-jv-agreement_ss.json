{
  "title": "Partnership/JV Agreement Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the Partnership or Joint Venture, e.g., 'Joint Venture Agreement for the development of...'"
    },
    "effective_date": {
      "type": "string",
      "format": "date",
      "description": "The date the agreement was executed or became effective."
    },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["General Partner", "Limited Partner", "Joint Venturer", "Managing Partner"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "partnership_details": {
      "type": "object",
      "properties": {
        "partnership_purpose": {
          "type": "string",
          "description": "The stated business purpose of the partnership or joint venture."
        },
        "management_structure": {
          "type": "string",
          "description": "A summary of how the partnership is managed (e.g., 'By the General Partner', 'By a Management Committee')."
        },
        "capital_contributions": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "partner_name": { "type": "string" },
              "contribution_amount": { "type": "number" },
              "contribution_description": { "type": "string" },
              "partnership_interest_pct": { "type": "number" }
            }
          }
        },
        "profit_loss_distribution": {
          "type": "string",
          "description": "A summary of how profits and losses are allocated among the partners."
        },
        "partnership_term": {
          "type": "string",
          "description": "The duration of the partnership (e.g., 'Perpetual', '5 years', 'Until project completion')."
        }
      }
    },
    "discovered_entities": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "entity_name": { "type": "string" },
          "entity_value": { "type": "string" },
          "context_summary": { "type": "string" }
        }
      }
    }
  },
  "required": ["contract_name", "effective_date", "parties"]
}