{
  "title": "CFD Election Results Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'Certificate of Election Results'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Certifying Official", "City Clerk"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "cfd_details": {
      "type": "object",
      "properties": {
        "cfd_number": { "type": "string" },
        "election_date": { "type": "string", "format": "date" },
        "votes_yes": { "type": "integer" },
        "votes_no": { "type": "integer" },
        "outcome": { "type": "string" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "cfd_details"]
}