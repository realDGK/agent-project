    { "name": "Placer County", "role": "Approving Agency" }
  ],
  "utility_service_details": {
    "utility_type": "Water",
    "map_reference": "Tentative Subdivision Map No. 1234",
    "verification_statement": "The River City Water District hereby verifies that a sufficient water supply is available to serve the proposed 500-lot subdivision."
  },
  "discovered_entities": []
