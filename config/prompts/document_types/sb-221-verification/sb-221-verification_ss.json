{
  "title": "SB 221 Verification Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'Verification of Sufficient Water Supply'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Water Supplier", "Approving Agency"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "utility_service_details": {
      "type": "object",
      "properties": {
        "utility_type": { "type": "string", "default": "Water" },
        "map_reference": { "type": "string" },
        "verification_statement": { "type": "string" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "utility_service_details"]
}