{
  "title": "Development Impact Fee Calculation Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'Development Impact Fee Estimate'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Applicant", "Public Agency", "City"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "fee_details": {
      "type": "object",
      "properties": {
        "project_name": { "type": "string" },
        "total_fee_amount": { "type": "number" },
        "fee_breakdown": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "fee_name": { "type": "string" },
              "amount": { "type": "number" }
            }
          }
        }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "parties", "fee_details"]
}