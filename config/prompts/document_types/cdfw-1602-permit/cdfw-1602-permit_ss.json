{
  "title": "CDFW 1602 Streambed Alteration Agreement Extraction",
  "type": "object",
  "properties": {
    "contract_name": {
      "type": "string",
      "description": "The name of the document, e.g., 'Lake and Streambed Alteration Agreement'."
    },
    "effective_date": { "type": "string", "format": "date" },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "role": {
            "type": "string",
            "enum": ["Operator", "Permittee", "Issuing Agency"]
          }
        },
        "required": ["name", "role"]
      }
    },
    "permit_details": {
      "type": "object",
      "properties": {
        "notification_number": { "type": "string" },
        "project_name": { "type": "string" },
        "authorized_activity": { "type": "string" },
        "mitigation_required": { "type": "string" },
        "term_of_agreement": { "type": "string" }
      }
    },
    "discovered_entities": {
        "type": "array",
        "items": {
            "type": "object",
            "properties": {
                "entity_name": { "type": "string" },
                "entity_value": { "type": "string" },
                "context_summary": { "type": "string" }
            }
        }
    }
  },
  "required": ["contract_name", "effective_date", "parties", "permit_details"]
}