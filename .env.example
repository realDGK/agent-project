# Google AI Configuration
GOOGLE_API_KEY=your_google_api_key_here

# Database Passwords
POSTGRES_PASSWORD=secure_postgres_password_here
NEO4J_PASSWORD=secure_neo4j_password_here

# Service URLs (internal Docker network)
DATABASE_URL=postgresql://agent_user:secure_postgres_password_here@postgres:5432/agent_orchestration
NEO4J_URI=bolt://neo4j:7687
QDRANT_URL=http://qdrant:6333
REDIS_URL=redis://redis:6379

# Optional: External API Keys for future integrations
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# Application Settings
ENVIRONMENT=development
LOG_LEVEL=INFO
SECRET_KEY=your_secret_key_here

# Feature Flags
ENABLE_PROVENANCE_TRACKING=true
ENABLE_CONFLICT_ENGINE=true
ENABLE_HIL_QUEUE=true
ENABLE_SEMANTIC_SEARCH=true